l":"https://tristanr-cogapp.github.io/canopy-demo/works/dottor-plus-quam-perfectus","openGraph":{"images":[{"url":"https://api.dc.library.northwestern.edu/api/v2/works/f912a581-d9e8-43d6-a7c8-a8d46eff7517/thumbnail","type":"image/jpeg","width":300,"height":300,"alt":"Dottor Plus Quam Perfectus"}]}},"referencingContent":[]},"__N_SSG":true}